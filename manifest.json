{
  "manifest_version": 2,

  "name": "Read-later bookmarks",
  "description": "Save links to read later.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },

  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",


  "permissions": [
    "webNavigation",
     "tabs"
  ]
}
